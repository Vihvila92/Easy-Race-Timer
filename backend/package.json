{
  "name": "easy-race-timer-backend",
  "version": "0.0.1-alpha",
  "private": true,
  "type": "commonjs",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/index.js",
    "migrate:up": "ts-node-dev --transpile-only src/migrate.ts up",
    "migrate:down": "ts-node-dev --transpile-only src/migrate.ts down",
    "migrate:status": "ts-node-dev --transpile-only src/migrate.ts status",
    "migrate:backfill": "ts-node-dev --transpile-only src/migrate.ts backfill",
    "migrate:rebaseline": "ts-node-dev --transpile-only src/migrate.ts rebaseline",
    "migrate:new": "ts-node-dev --transpile-only src/migrate.ts new",
    "seed": "ts-node-dev --transpile-only src/scripts/seed.ts",
    "test": "jest --passWithNoTests",
    "db:up": "docker compose -f ../deployment/docker-compose.dev.yml up -d",
    "db:down": "docker compose -f ../deployment/docker-compose.dev.yml down",
    "test:int": "cross-env DATABASE_URL=postgres://ert:ertpass@localhost:5432/ert_dev APP_ROLE=1 jest --runInBand src/__tests__/orgContext.test.js"
  },
  "dependencies": {
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "nanoid": "^5.1.5",
    "pg": "^8.11.5",
    "pino": "^9.8.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/node": "^24.2.1",
    "@types/pg": "^8.15.5",
    "@types/supertest": "^2.0.16",
    "cross-env": "^10.0.0",
    "jest": "^29.7.0",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.4.5"
  }
}
